<Window x:Class="DBApp.EditEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:DBApp"
        mc:Ignorable="d"
        Title="EditEntry" SizeToContent="WidthAndHeight">
    <StackPanel>
        <TextBox Name="Editable" Height="100" Width="400" TextWrapping="Wrap"/>
        <Button Height="20" Content="Применить" Name="Apply" Click="Apply_OnClick"/>
    </StackPanel>
</Window>
